<?xml version="1.0" encoding="utf-8"?>

<?xml-stylesheet type="text/xsl" href="style.xsl"?>

<page>
    <title>API References</title>

    <h2>Package libxml2.tree (xtree)</h2>

    <function name="parse">
        <argument  name="source">
            xml-data
        </argument>

        <return>
            libxml2:document object
        </return>

        <detail>
            parse input data and return result document
            <code>
                (xtree:parse "&lt;root&gt;&lt;a /&gt;&lt;b /&gt;&lt;c /&gt;&lt;/root&gt;")
            </code>
        </detail>
    </function>

    <function name="serialize">
        <argument name="object">
            xtree:document or xtree:node
        </argument>

        <argument name="target" type="optional">
            seralize target
        </argument>

        <argument name="target" type="key">
            seralize target
        </argument>
    </function>
</page>